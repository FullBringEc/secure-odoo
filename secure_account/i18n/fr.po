#. module: secure_account
#: code:addons/secure_account/models/account_invoice.py:41
#, python-format
msgid "The invoice %(invoice)s may not be cancelled or set to draft because it is validated."
msgstr "La facture %(invoice)s ne peut pas être annulée ou remise à brouillon car elle a déjà été validée."

#. module: secure_account
#: code:addons/secure_account/models/account_move.py:29
#, python-format
msgid "You may not delete the accounting entry %(entry)s because it posted."
msgstr "Vous ne pouvez pas supprimer la pièce comptable %(entry)s car elle est validée."

#. module: secure_account
#: code:addons/secure_account/models/account_move_line.py:38
#, python-format
msgid "You may not delete the accounting item %(line)s because it is bound to a posted accounting entry (%(entry)s)."
msgstr "Vous ne pouvez pas supprimer l'écriture %(line)s car elle est liée à une pièce (%(entry)s) validée."

#. module: secure_account
#: code:addons/secure_account/models/account_analytic_line.py:32
#, python-format
msgid "You may not delete the analytic line %(line)s because it is bound to a posted accounting entry (%(entry)s)."
msgstr "Vous ne pouvez pas supprimer l'écriture analytique %(line)s car elle est liée à une pièce comptable (%(entry)s) validée."

#. module: secure_account
#: code:addons/secure_account/models/account_invoice_line.py:35
#, python-format
msgid "You may not delete the invoice line %(line)s because the invoice (%(invoice)s) is validated."
msgstr "Vous ne pouvez pas supprimer la ligne de facture %(line)s car la facture (%(invoice)s) est validée."

#. module: secure_account
#: code:addons/secure_account/models/account_invoice_tax.py:35
